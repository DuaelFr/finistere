/* Header de la partie pro */

body.domain-2 #header_type{ background:#000 url(../images/header_pro.jpg) no-repeat; height:215px;}

#block-views-pro_accueil_agenda-block_1 { float:none; clear:both; }

/* Menu inline accueil espace pro */

body.domain-2 #box_profil{ margin-right:35px;}

body.domain-2 .bloc_1_2, body.domain-2  #block-views-pro_tagged_la_une-block_1 { width:328px; float:left;}

body.domain-2 .bloc_1_2 .last{ border:0;}

body.domain-2 .bloc_1_2 .content_box_agenda{ width:214px;}

body.domain-2 #box_profil h1{ margin-bottom:0;}

body.domain-2 #box_profil li{ height:43px; padding-left:20px; border-bottom:1px dashed #c8c8c8;}

body.domain-2 #box_profil li:hover{ background-color:#EFEFEF;}

body.domain-2 #box_profil li a{ font:bold 0.88em/42px Arial, Helvetica, sans-serif; text-transform:uppercase; color:#03a8ea; text-decoration:none;background: url(../images/sprite.png) no-repeat 0 -976px; display:block; padding-left:11px; }


/* Agenda */
body.domain-2 #conteneur_left_agenda .intro{ font-size:0.88em; line-height:24px; margin-bottom:10px;}

body.domain-2 #conteneur_left_agenda .box_decouvrez h2{ margin:0;}

body.domain-2 #conteneur_left_agenda .box_decouvrez h2 a{ font-size:1em;}

