

#conteneur_left.node-type-bon_plan .prix span + span + span + span{ position:absolute; color:#dddddd; font-size:0.75em; height:auto; bottom:12px; left:13px;}

#conteneur_left.node-type-bon_plan .prix{right: 329px; position: absolute; top: 118px; font-size:1em; background:none;}

#conteneur_left.node-type-bon_plan .prix span{ display:block; float:left;  height:87px;}

